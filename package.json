{
  "name": "node-red-contrib-botmaster",
  "version": "0.1.1",
  "description": "botmaster for node-red",
  "dependencies": {
    "botmaster": "^2.2.7",
    "botmaster-fulfill": "^2.1.4",
    "botmaster-fulfill-actions": "^0.2.2",
    "botmaster-session-ware": "^0.1.0",
    "node-red": "^0.16.2",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "localtunnel": "^1.8.2",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "watch": "nodemon",
    "tunnel": "( while true; do lt -p 1880 -s noderedcontribbotmaster; done; )",
    "start": "node-red --settings ./settings.js",
